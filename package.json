{
  "name": "angularMdStarterApp",
  "version": "0.1.0",
  "devDependencies": {
    "angular": "^1.6.0",
    "angular-ui-router": "^0.3.2",
    "autoprefixer": "~6.5.3",
    "babel-cli": "~6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "~6.2.8",
    "babel-preset-angular": "~6.0.15",
    "babel-preset-es2015": "~6.18.0",
    "commitizen": "^2.8.6",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "html-loader": "~0.4.4",
    "html-minify-loader": "~1.1.0",
    "html-webpack-plugin": "~2.24.1",
    "imports-loader": "^0.7.0",
    "jquery": "^3.1.1",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "json-loader": "~0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "ng-annotate-loader": "~0.2.0",
    "postcss-loader": "^1.2.1",
    "protractor": "^4.0.0",
    "raw-loader": "~0.5.1",
    "style-loader": "~0.13.1",
    "url-loader": "~0.5.7",
    "webpack": "^1.14.0",
    "webpack-dashboard": "git://github.com/FormidableLabs/webpack-dashboard",
    "webpack-dev-server": "~1.16.2",
    "webpack-livereload-plugin": "~0.9.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist dist/assets/",
    "build": "USE_CONFIG=production babel-node --harmony ./node_modules/webpack/bin/webpack --build",
    "build:local": "USE_CONFIG=local babel-node --harmony ./node_modules/webpack/bin/webpack --build",
    "postbuild": "cp -r src/assets/img dist/assets/ && cp -r src/build-files/. dist/",
    "start": "USE_CONFIG=local babel-node server.local.js",
    "start:production": "USE_CONFIG=production babel-node server.local.js",
    "jshint": "./node_modules/jshint/bin/jshint --reporter ./node_modules/jshint-stylish/stylish.js src/app",
    "precommit": "npm run jshint",
    "commit": "git-cz",
    "protractor": "protractor protractor.conf.js",
    "test": "USE_CONFIG=local-tests babel-node --harmony ./node_modules/karma/bin/karma start ./karma.conf.js",
    "test-remote": "USE_CONFIG=remote-tests babel-node --harmony ./node_modules/karma/bin/karma start ./karma.conf.js",
    "test-e2e": "./node_modules/protractor/bin/protractor protractor.conf.js",
    "travis-flow": "npm run build && npm run test && node server.test.js & npm run test-e2e"
  },
  "dependencies": {
    "express": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
